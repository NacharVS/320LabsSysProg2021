<Page x:Class="WpfCleanerBD.CleanerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfCleanerBD"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CleanerPage">

    <Grid>
        <Button x:Name="Back" Content="Back" Width="150" Height="20" HorizontalAlignment="Right" VerticalAlignment="Top"  Click="Back_Click"/>
        <Button x:Name="Random" Content="Random" Width="150" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center"  Click="Random_Click"/>
        <TextBlock x:Name="text_Name"/>
    </Grid>
</Page>
